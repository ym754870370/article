// fade 动画效果
.fade-enter-active
.fade-leave-active
    transition opacity .29s
    webkit-transform translateZ(0)
    transform translateZ(0)
    -webkit-backface-visibility hidden
    -webkit-perspective 1000
.fade-enter
.fade-leave-to
    opacity 0
    webkit-transform translateZ(0)
    transform translateZ(0)
    -webkit-backface-visibility hidden
    -webkit-perspective 1000